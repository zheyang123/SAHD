<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asingtesting">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="S.A.H.D"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".edidtprofile"></activity>
        <activity android:name=".profile" />
        <activity android:name=".customerservices" />
        <activity android:name=".aboutus" />
        <activity
            android:name=".AddToCart"
            android:label="Add To Cart" />
        <activity
            android:name=".getProductInfo"
            android:label="Product Info" />
        <activity
            android:name=".ProductListMain"
            android:label="Product List"
            android:parentActivityName=".display_product_list" />
        <activity
            android:name=".Company_Main_Page"
            android:label="Main Page" />
        <activity
            android:name=".CompanyListMain"
            android:label="Company Register" />
        <activity
            android:name=".historymain"
            android:label="Order History" />
        <activity
            android:name=".orderdetails"
            android:label="Order Detail"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".display_product_list"
            android:label="Product List" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>